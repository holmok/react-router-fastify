{
  "name": "react-router-fastify",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonsmith/react-router-fastify"
  },
  "author": "Simon Smith <simonsmith@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch --no-warnings --env-file=.env ./server/index.ts",
    "clean": "rm -rf dist && rm -rf build",
    "format": "prettier --write .",
    "compile:server": "tsc --project ./tsconfig.server.json",
    "compile:client": "react-router build",
    "compile": "yarn compile:server && yarn compile:client",
    "build": "yarn && yarn format && yarn lint && yarn clean && yarn compile",
    "lint": "eslint",
    "tsc": "tsc",
    "typegen": "react-router typegen",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@mcansh/remix-fastify": "^4.0.8",
    "@react-router/fs-routes": "^7.6.0",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "@tanstack/react-query": "^5.67.3",
    "fastify": "^5.2.1",
    "isbot": "^5",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.3.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.27.0",
    "eslint-config-love": "^120.0.0",
    "eslint-config-prettier": "^10.1.5",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}